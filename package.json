{
  "name": "connect-platform",
  "version": "1.0.0",
  "description": "Connect - Explainable matching platform for Korea's innovation ecosystem",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "scraper": "tsx lib/scraping/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:auth-setup": "npx playwright test auth-manual.setup.ts --headed --project=setup",
    "test:e2e:auth-verify": "npx playwright test auth-verify.spec.ts --project=chromium",
    "test:e2e:full": "npx playwright test --project=chromium --project=\"Mobile Chrome - Pixel 5\"",
    "test:e2e:prod": "PLAYWRIGHT_BASE_URL=https://connectplt.kr npx playwright test",
    "docker:build": "docker compose -f docker-compose.production.yml build",
    "docker:up": "docker compose -f docker-compose.production.yml up -d",
    "docker:down": "docker compose -f docker-compose.production.yml down",
    "docker:restart": "docker compose -f docker-compose.production.yml restart",
    "docker:logs": "docker compose -f docker-compose.production.yml logs -f",
    "docker:ps": "docker compose -f docker-compose.production.yml ps",
    "docker:clean": "docker compose -f docker-compose.production.yml down -v",
    "deploy": "./scripts/deploy.sh",
    "rollback": "./scripts/rollback.sh",
    "backup": "./scripts/backup.sh",
    "health": "./scripts/health-monitor.sh",
    "loadtest:smoke": "k6 run scripts/loadtest.js --env TEST_TYPE=smoke",
    "loadtest:load": "k6 run scripts/loadtest.js --env TEST_TYPE=load",
    "loadtest:stress": "k6 run scripts/loadtest.js --env TEST_TYPE=stress"
  },
